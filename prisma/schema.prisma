generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/client"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Token {
  id    Int     @id @default(autoincrement())
  value String  @unique

  @@map("tokens") 
}

model Establishment {
  id         Int    @id @default(autoincrement())
  name       String
  cnpj       String @unique
  email      String @unique
  password   String
  createTime DateTime @default(now()) @map("create_time")

  @@map("establishments")
}
